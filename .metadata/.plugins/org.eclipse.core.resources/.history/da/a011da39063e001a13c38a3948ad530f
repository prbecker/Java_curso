import java.util.Scanner;

public class ex4 


	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		int A;
		int B;
		int C;
		int D;
		int dif;
				
		Scanner sc = new Scanner(System.in);
				
		A = sc.nextInt();
		B = sc.nextInt();
		C = sc.nextInt();
		D = sc.nextInt();
				
		dif = (A*B) - (C*D);
		System.out.println("DIFERENCA = " + dif);
				
		sc.close();
	}
}
