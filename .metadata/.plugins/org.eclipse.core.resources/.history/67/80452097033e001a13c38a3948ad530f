import java.util.Scanner;

public class ex1 
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		int x;
		int y;
		int soma;
		Scanner sc = new Scanner(System.in);
		
		x = sc.nextInt();
		y = sc.nextInt();	
		
		soma = x + y;
		System.out.println("SOMA = " + soma);
	}

}
